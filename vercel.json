{
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000 ",
    "builds": [
        {
            "src": "Backend/FastAPI/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Backend/FastAPI/main.py"
        }
    ]
}